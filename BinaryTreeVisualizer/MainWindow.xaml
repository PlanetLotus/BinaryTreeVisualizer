<Window x:Class="BinaryTreeVisualizer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BinaryTreeVisualizer"
        mc:Ignorable="d"
        Title="MainWindow" Height="480" Width="640">
    <Grid Width="640" Height="480">
        <TextBox x:Name="val" HorizontalAlignment="Left" Height="23" Margin="547,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="insert" Content="Insert " HorizontalAlignment="Left" Margin="547,40,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="delete" Content="Delete" HorizontalAlignment="Left" Margin="547,67,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="findMin" Content="Find Min" HorizontalAlignment="Left" Margin="547,94,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="findMax" Content="Find Max" HorizontalAlignment="Left" Margin="547,121,0,0" VerticalAlignment="Top" Width="75"/>
        <Grid Name="treeGrid" Margin="0,0,98,0">
        </Grid>
    </Grid>
</Window>
